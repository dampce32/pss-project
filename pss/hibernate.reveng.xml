<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
    <table name="t_product" schema="dbo" catalog="pss"
		class="Product">
	</table>
	<table name="t_employee" schema="dbo" catalog="pss"
		class="Employee">
	</table>
	<table name="t_warehouse" schema="dbo" catalog="pss"
		class="Warehouse">
	</table>
	<table name="t_invoicetype" schema="dbo" catalog="pss"
		class="InvoiceType">
	</table>
	<table name="t_buy" schema="dbo" catalog="pss" class="Buy">
	</table>
	<table name="t_buydetail" schema="dbo" catalog="pss"
		class="BuyDetail">
	</table>
	<table name="t_customer" catalog="pss" class="Customer"></table>
	<table name="t_deliver" catalog="pss" class="Deliver"></table>
	<table name="t_deliverdetail" catalog="pss" class="DeliverDetail"></table>
	<table name="t_deliverreject" catalog="pss" class="DeliverReject"></table>
	<table name="t_deliverrejectdetail" catalog="pss"
		class="DeliverRejectDetail">
	</table>
	<table name="t_express" catalog="pss" class="Express"></table>
	<table name="t_sale" catalog="pss" class="Sale"></table>
	<table name="t_saledetail" catalog="pss" class="SaleDetail"></table>
	<table name="t_pay" catalog="pss" class="Pay">
		<column name="supplierId" property="supplier"></column>
		<column name="bankId" property="bank"></column>
		<column name="employeeId" property="employee"></column>
	</table>
	<table name="t_paydetail" catalog="pss" class="PayDetail"></table>
	<table name="t_prefix" catalog="pss" class="Prefix"></table>
	<table name="t_reportconfig" catalog="pss" class="ReportConfig"></table>
	<table name="t_prepay" catalog="pss" class="Prepay"></table>
	<table name="t_bankstatements" catalog="pss"
		class="BankStatements">
	</table>
	<table name="t_expense" catalog="pss" class="Expense"></table>
	<table name="t_income" catalog="pss" class="Income"></table>
	<table name="t_reportparam" catalog="pss" class="ReportParam"></table>
	<table name="t_reportparamconfig" catalog="pss"
		class="ReportParamConfig">
	</table>
	<table name="t_defaultpackage" catalog="pss"
		class="DefaultPackage">
	</table>
	<table name="t_package" catalog="pss" class="Package"></table>
	<table name="t_packagedetail" catalog="pss" class="PackageDetail"></table>
	<table name="t_split" catalog="pss" class="Split"></table>
	<table name="t_splitdetail" catalog="pss" class="SplitDetail"></table>
</hibernate-reverse-engineering>